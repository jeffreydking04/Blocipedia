<%= form_for @wiki do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, class: 'form-control' %>

  <%= f.label :body %>
  <%= f.text_area :body, class: 'form-control' %><br>

  <div class="form_group">
    <%= f.label :private, class: 'checkbox' do %>
      <%= f.check_box :private %> Private topic
    <% end %>
  </div>

  <%= f.submit "Save", class: 'btn seahawk'%>
<% end %>